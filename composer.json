    "keywords": [
        "yii2",
        "extension",
        "widget",
        "export",
        "spreadsheet",
        "html",
        "csv",
        "text",
        "pdf",
        "json",
        "xls",
        "xlsx",
        "OpenXML"
    ],
    "homepage": "https://github.com/frmaxm/yii2-export",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
    "require": {
        "kartik-v/yii2-grid": "~3.0",
        "phpoffice/phpexcel": "~1.8.0"
